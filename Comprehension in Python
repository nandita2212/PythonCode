num={1,2,3,4,5}
output=list[]
for i in num:
  output.append(i**2)
print(output)

num={1,2,3,4,5}
output=[i**2 for i in num]
print(output)

//Removing Duplicates...............
def removeduplicates(list1):
  list2=[]
  for i in list1:
    if i in not list2:
      list2.append(i)
  return list2
  
def removeduplicates([1,2,3,4,5,6,7,7,8])
